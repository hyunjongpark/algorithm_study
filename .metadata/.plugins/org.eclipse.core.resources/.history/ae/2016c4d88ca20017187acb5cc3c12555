package DP;

import java.io.BufferedReader;
import java.io.FileInputStream;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.StringTokenizer;

public class WineTasting {
	static int T;
	static int[][] dpt;
	static int[] arr;

	public static void main(String[] args) throws NumberFormatException, IOException {
		System.setIn(new FileInputStream("winetasting"));
		InputStreamReader isr = new InputStreamReader(System.in);
		BufferedReader br = new BufferedReader(isr);
		StringTokenizer token;

		T = Integer.parseInt(br.readLine());
		dpt = new int[T][6];
		arr = new int[T];

		
		for (int i = 0; i < T; i++) {
			int v = Integer.parseInt(br.readLine());
			arr[i] = v;
			updateTable(v, i);
			
			printTable();
			System.out.println();
		}
	}

	public static void updateTable(int v, int row) {
		if (row == 0) {
			for (int i = 0; i < 6; i++) {
				switch (i) {
				/*
				 * 0: 001 
				 * 1: 010 
				 * 2: 011 
				 * 3: 100 
				 * 4: 101 
				 * 5: 110
				 */
				case 0: {
					dpt[row][i] = v;
					break;
				}
				case 1: {
					dpt[row][i] = 0;
					break;
				}
				case 2: {
					dpt[row][i] = v;
					break;
				}
				case 3: {
					dpt[row][i] = 0;
					break;
				}
				case 4: {
					dpt[row][i] = v;
					break;
				}
				case 5: {
					dpt[row][i] = 0;
					break;
				}

				}
			}
		}
		
		else if (row == 1) {
			for (int i = 0; i < 6; i++) {
				switch (i) {
				/*
				 * 0: 001 
				 * 1: 010 
				 * 2: 011 
				 * 3: 100 
				 * 4: 101 
				 * 5: 110
				 */
				case 0: {
					dpt[row][i] = v;
					break;
				}
				case 1: {
					dpt[row][i] = arr[row-1];
					break;
				}
				case 2: {
					dpt[row][i] = arr[row-1] + v;
					break;
				}
				case 3: {
					dpt[row][i] = 0;
					break;
				}
				case 4: {
					dpt[row][i] = v;
					break;
				}
				case 5: {
					dpt[row][i] = arr[row-1];
					break;
				}

				}
			}
		}

	}

	public static void printTable() {
		for (int i = 0; i < T; i++) {
			for (int j = 0; j < 6; j++) {
				System.out.print(dpt[i][j] + "	");
			}
			System.out.println();
		}
	}

}
